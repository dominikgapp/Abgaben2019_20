
public class Observer implements IObserver {

	private int IntValue; 
	
	
	
	public Observer(int sv) {
		this.IntValue = sv; 
	}
	
	@Override
	public int getIntValue() {
		return this.IntValue; 
		
	}
	
	@Override
	public void update(int i) {
		System.out.println("value has changed: " +i);	
	}
	
	
}

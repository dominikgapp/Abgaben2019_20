package parkgarage;

public class Garage {

	
	private int [] a; 
	private int wert=0;
	private int altwert;
	private int max = 0; 
	public Garage(int[] array)
	{
		a = array; 
	}
	
	
	public static void checknumbers(int[] array) 
	{
		for (int i= 0; i<= array.length; i++)
		{
			if(array[i]>0)
			{
				wert+=1;
			}
			else
			{
				wert-=1;
			}
			
			if (wert > max)
			{
				max = wert; 
			}
			
			
		}
		
		System.out.println(max);
		
		
	}

	
}
